{
  "name": "keystone-app",
  "version": "1.0.2",
  "private": true,
  "main": "keystone.ts",
  "scripts": {
    "dev": "keystone dev --no-db-push",
    "start": "keystone start",
    "build": "keystone build",
    "postinstall": "keystone build --no-ui --frozen",
    "test": "echo 'No tests'",
    "ship": "npx shipit production deploy",
    "deploy:prod-old": "pm2 deploy ecosystem.config.js production",
    "deploy:prod": "npm install && npm run build && npm run migrate:deploy && NODE_ENV=\"production\" pm2 startOrReload ecosystem.config.js --update-env",
    "migrate:dev": "keystone prisma migrate dev",
    "migrate:deploy": "keystone prisma migrate deploy"
  },
  "dependencies": {
    "@keystone-6/auth": "^8.0.0",
    "@keystone-6/cloudinary": "^8.0.0",
    "@keystone-6/core": "^6.3.0",
    "@keystone-6/fields-document": "^9.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "slugify": "^1.6.6",
    "typescript": "^4.9.5"
  },
  "overrides": {
    "next": "13.4.12"
  },
  "devDependencies": {
    "shipit-cli": "^5.3.0",
    "shipit-deploy": "^5.3.0",
    "shipit-shared": "^4.4.2"
  }
}